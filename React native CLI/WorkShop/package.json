    "react-native-vector-icons": "^9.0.0",
    "tailwind-react-native-classnames": "^1.5.1",
    "tailwind-rn": "^4.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
